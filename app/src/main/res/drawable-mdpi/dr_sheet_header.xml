<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ABC"/>
        </shape>
    </item>
    <item android:bottom="1px" android:left="0px" android:right="1px" android:top="0px">
        <shape android:shape="rectangle">
            <gradient android:angle="90"
                android:startColor="#D4D8DC"
                android:endColor="#E4E8EC"
                android:type="linear"/>
        </shape>
    </item>
</layer-list>
